{
  "include": {
    "prefix": "include",
    "body": ["include \"$1.bang\""]
  },
  "let": {
    "prefix": "let",
    "body": ["let $1 in", "\t$2", "end"]
  },
  "memory": {
    "prefix": "memory",
    "body": ["memory $1 end"]
  },
  "const": {
    "prefix": "const",
    "body": ["const $1 end"]
  },
  "while": {
    "prefix": "while",
    "body": ["$1 while $2 do", "\t$3", "end"]
  },
  "if": {
    "prefix": "if",
    "body": ["$1 if", "\t$2", "end"]
  },
  "if*": {
    "prefix": "if*",
    "body": ["else $1 if*", "\t$2", "end"]
  },
  "todo": {
    "prefix": "todo",
    "body": ["here eputs \": TODO: $1\\n\" eputs", "1 exit"]
  },
  "error": {
    "prefix": "error",
    "body": ["here eputs \": ERROR: $1\n\" eputs", "1 exit"]
  },
  "main": {
    "prefix": "main",
    "body": ["include \"std.bang\"", "", "proc main in", "\t$1", "end"]
  },
  "proc": {
    "prefix": "proc",
    "body": ["proc $1 in", "\t$1", "end"]
  }
}
